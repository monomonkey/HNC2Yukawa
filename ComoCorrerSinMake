cd facdes2Y && gcc -c ./math/math_aux.c -o ./objects/math_aux.o -lgsl -lgslcblas -lm && gcc -c ./structures/structures.c -o ./structures/structures.o -lgsl -lgslcblas -lm && gcc -c facdes2Y.c -o ./objects/facdes2Y.o -lgsl -lgslcblas -lm && cd .. && ar rcs libfacdes2Y.a ./facdes2Y/objects/*.o && gcc -c mainFile.c -o mainFile.o -L. -lfacdes2Y -lm -lgsl -lgslcblas && gcc mainFile.o -L. -lfacdes2Y -o main.exe -lm -lgsl -lgslcblas && ./main.exe
